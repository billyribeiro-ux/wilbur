{
  "name": "revolution-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:network": "vite --host",
    "dev:port": "vite --port",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src tests scripts --ext .ts,.tsx,.js,.mjs",
    "lint:fix": "eslint src tests scripts --ext .ts,.tsx,.js,.mjs --fix",
    "test": "vitest",
    "test:unit": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:production": "vitest --run --config vitest.config.production.ts",
    "test:e2e": "playwright test",
    "test:load": "k6 run tests/load-test.js",
    "test:security": "npm audit && snyk test",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:security",
    "validate:production": "tsx scripts/production-validation.ts",
    "preflight": "bash scripts/pre-flight-check.sh",
    "validate:db": "tsx scripts/validate-database-queries.ts",
    "validate:db:watch": "tsx scripts/validate-database-queries.ts --watch",
    "validate:prod": "tsx scripts/validate-production-db.ts",
    "db:schema": "tsx scripts/validate-production-db.ts",
    "validate:live": "tsx scripts/validate-live-queries.ts",
    "validate:full": "npm run validate:prod && npm run validate:live",
    "investigate:db": "tsx scripts/investigate-database.ts",
    "eradicate:nulls": "tsx scripts/eradicate-nulls.ts",
    "eradicate:nulls:enhanced": "tsx scripts/enhanced-eradicate-nulls.ts",
    "eradicate:nulls:final": "tsx scripts/final-eradicate-nulls.ts",
    "check:nulls": "grep -r '\\bnull\\b' src/ --include='*.ts' --include='*.tsx' | wc -l",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@daily-co/daily-js": "^0.87.0",
    "@daily-co/daily-react": "^0.25.0",
    "@fluentui/react-components": "^9.75.0",
    "@fluentui/react-icons": "^2.0.250",
    "@fortawesome/fontawesome-svg-core": "^7.1.1",
    "@fortawesome/free-brands-svg-icons": "^7.1.1",
    "@fortawesome/free-regular-svg-icons": "^7.1.1",
    "@fortawesome/free-solid-svg-icons": "^7.1.1",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@supabase/supabase-js": "^2.79.0",
    "date-fns": "^4.2.0",
    "dompurify": "^3.4.0",
    "dotenv": "^17.3.0",
    "framer-motion": "^12.25.0",
    "immer": "^10.2.1",
    "livekit-client": "^2.17.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.550.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.11.0",
    "web-vitals": "^5.2.0",
    "zustand": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/css": "^0.15.0",
    "@eslint/js": "^9.41.0",
    "@eslint/json": "^0.15.0",
    "@eslint/markdown": "^7.6.0",
    "@playwright/test": "^1.51.0",
    "@testing-library/jest-dom": "^6.10.0",
    "@testing-library/react": "^16.4.0",
    "@testing-library/user-event": "^14.7.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.22",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitest/coverage-v8": "^4.1.0",
    "autoprefixer": "^10.5.0",
    "eslint": "^9.41.0",
    "eslint-plugin-react": "^7.38.0",
    "eslint-plugin-react-hooks": "^5.3.0",
    "glob": "^11.0.0",
    "globals": "^16.6.0",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.7",
    "puppeteer": "^24.32.0",
    "tailwindcss": "^3.5.0",
    "tsx": "^4.22.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.0",
    "vitest": "^4.1.0"
  }
}
