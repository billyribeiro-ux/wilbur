{
  "name": "revolution-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:network": "vite --host",
    "dev:port": "vite --port",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src tests scripts --ext .ts,.tsx,.js,.mjs",
    "lint:fix": "eslint src tests scripts --ext .ts,.tsx,.js,.mjs --fix",
    "test": "vitest",
    "test:unit": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:production": "vitest --run --config vitest.config.production.ts",
    "test:e2e": "playwright test",
    "test:load": "k6 run tests/load-test.js",
    "test:security": "npm audit && snyk test",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:security",
    "validate:production": "tsx scripts/production-validation.ts",
    "preflight": "bash scripts/pre-flight-check.sh",
    "validate:db": "tsx scripts/validate-database-queries.ts",
    "validate:db:watch": "tsx scripts/validate-database-queries.ts --watch",
    "validate:prod": "tsx scripts/validate-production-db.ts",
    "db:schema": "tsx scripts/validate-production-db.ts",
    "validate:live": "tsx scripts/validate-live-queries.ts",
    "validate:full": "npm run validate:prod && npm run validate:live",
    "investigate:db": "tsx scripts/investigate-database.ts",
    "eradicate:nulls": "tsx scripts/eradicate-nulls.ts",
    "eradicate:nulls:enhanced": "tsx scripts/enhanced-eradicate-nulls.ts",
    "eradicate:nulls:final": "tsx scripts/final-eradicate-nulls.ts",
    "check:nulls": "grep -r '\\bnull\\b' src/ --include='*.ts' --include='*.tsx' | wc -l",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@daily-co/daily-js": "^0.85.0",
    "@daily-co/daily-react": "^0.24.0",
    "@fluentui/react-components": "^9.72.3",
    "@fluentui/react-icons": "^2.0.233",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@supabase/supabase-js": "^2.76.1",
    "@types/react-router-dom": "^5.3.3",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "immer": "^10.2.0",
    "livekit-client": "^2.15.13",
    "lodash": "^4.17.21",
    "lucide-react": "^0.546.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.1",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.23",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^4.0.5",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^10.0.0",
    "globals": "^16.5.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "puppeteer": "^24.30.0",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.1.12",
    "vitest": "^4.0.5"
  }
}
